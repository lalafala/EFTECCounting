<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.efteccounting">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ico"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".activity.MyApplication"
        android:networkSecurityConfig="@xml/network_config"
        >
    <activity android:name=".activity.LaunchActivity" android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.LoginActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.MainActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.StorageInActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.StorageOutActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.CountActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.StorageOutBackActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.StorageInBackActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.SearchActivity">
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:name=".activity.StorageOutBackUpActivity">
        </activity>
    </application>
</manifest>